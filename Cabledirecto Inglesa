<page>$doregex[geturl]</page>
</regex>
<regex>
<name>geturl</name>
<expres><![CDATA[#$pyFunction
import requests, re, xbmcgui, xbmc
def GetLSProData(page_data,Cookie_Jar,m,url = 'https://www.youtube.com/watch?v=y60wDzZt8yg'):
 session = requests.Session()
 session.cookies = Cookie_Jar
 source = session.get(url)
 m3u8 = re.findall('"hlsvp":"(.*?)"', source.text)[0].replace('\/','/').replace('%2C',',').replace('%3A',':')
 return m3u8
]]></expres>
<page></page>
<cookieJar></cookieJar>
</regex>

import os,socket
import urllib
from selenium import webdriver
from selenium.webdriver.common.desired_capabilities import DesiredCapabilities
from selenium.webdriver.support.wait import WebDriverWait
options = webdriver.ChromeOptions()
options.add_argument('--disable-logging')
# disable images
prefs = {"profile.managed_default_content_settings.images":2}
options.add_experimental_option("prefs",prefs)
capabilities = options.to_capabilities()
socket.setdefaulttimeout(30)
driver = webdriver.Chrome(desired_capabilities=capabilities)
def jwplayer_to_kodi(config, referer, user_agent):
    rtmp_securetoken = ''
    if 'rtmp' in config:
        if 'securetoken' in config['rtmp']:
            rtmp_securetoken = ' token='+config['rtmp']['securetoken']
    elif 'token' in config:
        rtmp_securetoken = ' token='+config['token']
        
    if not 'flashplayer' in config:
        config['flashplayer'] = 'http://p.jwpcdn.com/6/12/jwplayer.flash.swf'
    if 'streamer' in config:
        if len(config['streamer']) < 5:
            rtmp = '{streamer[0]}/{file[0]} swfUrl={flashplayer}{securetoken} pageUrl={referer}'
        else:
            rtmp = '{streamer}/{file} swfUrl={flashplayer}{securetoken} pageUrl={referer}'
        return rtmp.format(referer=referer, securetoken=rtmp_securetoken, **config)
    else:
        if 'file' in config:
            if 'rtmp' in config['file']:
                rtmp = '{file} swfUrl={flashplayer}{securetoken} pageUrl={referer}'
                return rtmp.format(referer=referer, securetoken=rtmp_securetoken, **config)
            else:
                return '{file}|Referer={referer}&User-Agent={user_agent}'.format(referer=urllib.quote(referer),
                                                                                 user_agent=urllib.quote(user_agent),
                                                                                 file=config['file'])
        else:
            return '{file}|Referer={referer}&User-Agent={user_agent}'.format(referer=urllib.quote(referer),
                                                                             user_agent=urllib.quote(user_agent),
                                                                             file=config['playlist'][0]['sources'][0]['file'])
def GetLSProData(page_data,Cookie_Jar,m):
    driver.get(m['page'])
    count = 0
    while (count == 0):
        try:
            alert = driver.switch_to.alert
            alert.dismiss()
        except:
            pass
        frame_list = driver.find_elements_by_tag_name('iframe')
        for index, frame in enumerate(frame_list):
            if (frame.size['width'] > 400 and frame.size['height'] > 400):
                driver.switch_to.frame(frame)
                print driver.execute_script("return window.location.href;")
                try:
                    jwplayer_config = driver.execute_script("return jwplayer().config;")
                    if jwplayer_config:
                        referer    = driver.execute_script("return window.location.href;")
                        user_agent = driver.execute_script("return navigator.userAgent;")
                        driver.quit()
                        return urllib.quote(jwplayer_to_kodi(jwplayer_config, referer, user_agent))
                except:
                    break
            else:
                if index+1 >= len(frame_list):
                    driver.quit()
                    return None
    driver.quit()
    return None
 
]]></expres>
    <page>[makelist.param1]</page>
